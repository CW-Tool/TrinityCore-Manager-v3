<Window x:Class="TrinityCore_Manager.Views.AddEditItemWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:viewModels="clr-namespace:TrinityCore_Manager.ViewModels"
        xmlns:catel="http://catel.codeplex.com"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        Title="Add Edit Item" Height="405" Width="681" Icon="/TrinityCore-Manager;component/Resources/edit.ico">

    <i:Interaction.Behaviors>
        <catel:WindowBehavior x:Name="mvvmBehavior" ViewModelType="{x:Type viewModels:AddEditItemViewModel}"/>
    </i:Interaction.Behaviors>

    <Grid>

        <xctk:Wizard HorizontalAlignment="Left" x:Name="setupWizard" VerticalAlignment="Top" Height="405" Width="681" FinishButtonClosesWindow="False" HelpButtonVisibility="Hidden" />

    </Grid>
</Window>
